{
    "manifest_version": 2,
    "name": "English Tutor",
    "description": "Extantion for learn english",
    "version": "1.0.0",
    "author": "Oleh Halay",

    "browser_action": {
        "default_popup": "index.html",
        "default_title": "English Tutor"
    },
    
   "permissions": [
       "contextMenus",
       "https://od-api.oxforddictionaries.com/",
       "identity",
       "identity.email"
   ],
   "icons" :{
       "16": "assets/icons/vocabulary.png",
       "48": "assets/icons/vocabulary.png",
       "128": "assets/icons/vocabulary.png"
   },
   "background": { 
      "scripts": [
          "assets/scripts/background.js",
          "assets/scripts/server.js"
          ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
        "assets/css/*",
        "assets/js/*",
        "assets/fonts/*"
    ],
    "oauth2" : {
        "client_id":"312985462032-9pv2mpvo2dtl6b0jqc8s3hvjmt6s26r2.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/plus.me"
        ]
    },
    "key" : "",
    "options_ui": {
        "page": "assets/options/options.html",
        "chrome_style": true
    }
}