{
    "manifest_version": 2,
    "name": "Eanglish Tutor",
    "description": "Extantion for learn english",
    "version": "1.0.0",
    "author": "Oleh Halay",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    
   "permissions": [],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
        "assets/css/*",
        "assets/js/*",
        "assets/fonts/*"
    ]
}